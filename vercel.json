{
  "rewrites": [
    {
      "source": "/login",
      "destination": "/login.tsx"
    },
    {
      "source": "/",
      "destination": "/index"
    },
    {
      "source": "/Bills",
      "destination": "/Bills/index.tsx"
    },
    {
      "source": "/Bills/billManagement",
      "destination": "/Bills/billManagement.tsx"
    },
    {
      "source": "/Bills/edit/:id",
      "destination": "/Bills/editBill.tsx"
    },
    {
      "source": "/Bills/create",
      "destination": "/Bills/createBill.tsx"
    },
    {
      "source": "/creditors",
      "destination": "/creditors/index.tsx"
    },
    {
      "source": "/creditors/create",
      "destination": "/creditors/createCreditor.tsx"
    },
    {
      "source": "/creditors/:id",
      "destination": "/creditors/singleCreditors.tsx"
    },
    {
      "source": "/creditors/list",
      "destination": "/creditors/creditorsList.tsx"
    },
    {
      "source": "/payment",
      "destination": "/payment/index.tsx"
    },
    {
      "source": "/payment/create",
      "destination": "/payment/createPayment.tsx"
    },
    {
      "source": "/payment/edit/:id",
      "destination": "/payment/editPayment.tsx"
    },
    {
      "source": "/payment/list",
      "destination": "/payment/paymentList.tsx"
    },
    {
      "source": "/debt",
      "destination": "/debt/index.tsx"
    },
    {
      "source": "/debt/create",
      "destination": "/debt/createDebt.tsx"
    },
    {
      "source": "/debt/edit/:id",
      "destination": "/debt/editDebt.tsx"
    },
    {
      "source": "/debt/list",
      "destination": "/debt/debtList.tsx"
    },
    {
      "source": "/test",
      "destination": "/test.tsx"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
